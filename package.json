{
  "name": "expo-and-typescript",
  "version": "0.0.0",
  "description": "Showcase of an Expo app coded in TypeScript.",
  "author": {
    "email": "jan@aagaard.net",
    "name": "Jan Aagaard"
  },
  "private": true,
  "license": "MIT",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "yarn run start --android",
    "check-types": "tsc",
    "ios": "yarn run start --ios",
    "lint": "eslint --format unix \"**/*.ts\" \"**/*.tsx\"",
    "start": "yarn install && expo start",
    "validate": "yarn install --frozen-lockfile && yarn run lint && yarn run check-types"
  },
  "engines": {
    "node": ">=10",
    "yarn": ">=1.1"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "expo": "^43.0.1",
    "expo-analytics-amplitude": "~11.0.3",
    "expo-apple-authentication": "~4.0.3",
    "expo-asset": "~8.4.3",
    "expo-av": "~10.1.2",
    "expo-barcode-scanner": "~11.1.2",
    "expo-blur": "~10.0.3",
    "expo-camera": "~12.0.3",
    "expo-constants": "~12.1.3",
    "expo-facebook": "~12.0.3",
    "expo-font": "~10.0.3",
    "expo-linear-gradient": "~10.0.3",
    "expo-local-authentication": "~12.0.1",
    "expo-sensors": "~11.0.3",
    "react": "17.0.1",
    "react-native": "0.64.2",
    "react-native-gesture-handler": "~1.10.3",
    "react-native-maps": "0.28.0",
    "react-native-reanimated": "~2.2.3",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.8.0",
    "react-native-svg": "12.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@types/react": "~17.0.33",
    "@types/react-native": "~0.64.18",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-preset-expo": "8.5.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "expo-cli": "^4.12.10",
    "prettier": "^2.4.1",
    "typescript": "~4.3.5"
  }
}
